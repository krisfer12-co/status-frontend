{
  "expo": {
    "name": "STATUS",
    "slug": "status-app",
    "owner": "krisfer12",
    "version": "1.0.20",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "status",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.krisfer12.statusapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0f172a"
      },
      "package": "com.krisfer12.statusapp",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "plugins": [
      "expo-router",
      "expo-camera",
      "expo-image-picker",
      "expo-asset"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "fe0e7f34-5ef8-4b8e-a15c-2562eb9fe26c"
      }
    }
  }
}
